<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.music.persistence.SingerSongMapper">
	<select id="findAll" resultType="SingerSong">
		select ss.songname from singersong ss
		where ss.singerid=#{singerid}
	</select>
</mapper>
